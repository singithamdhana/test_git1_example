pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'building stage' 
            }
        }
        stage('test') {
            steps {
                echo 'testing stage' 
            }
        }
        stage('Deploy') {
            steps {
                echo 'deploying stage' 
                echo 'testing trigger'
            }
        }   
    }
}
